<map
        id="map"
        latitude="{{location.latitude}}"
        longitude="{{location.longitude}}"
        scale="{{scale}}"
        setting="{{setting}}"
        enable-overlooking="{{isOverLooking}}"
        enable-3D="{{is3D}}"
        markers="{{markers}}"
>
    <cover-view class="icon-container">
        <cover-image class="round" src="{{avatarURL?avatarURL:'/resources/account.png'}}" bindtap="onMyTrips"></cover-image>
        <cover-image
                src="/resources/my_location.png"
                bindtap="onMyLocationTap"
        ></cover-image>
    </cover-view>
    <cover-view class="btn-container bg-grey round" bindtap="onScanClicked">
        <cover-view >扫码领枪</cover-view>
    </cover-view>
</map>
<modal id="licModal"
       title="身份认证" contents="尚未验证,需要身份认证才能领枪"></modal>
<modal id="tripModal"
       title="行程中" contents="当前有正在进行的行程，将跳转到行程页面"></modal>
